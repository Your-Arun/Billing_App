{
  "expo": {
    "name": "frontend",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",

    "android": {
      "package": "com.urarun0211.frontend",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#E6F4FE"
      },
      "permissions": [
        "CAMERA",
        "READ_MEDIA_IMAGES",
        "POST_NOTIFICATIONS"
      ]
    },

    "plugins": [
      "expo-router",
      "expo-camera",
      "expo-media-library",
      "expo-notifications"
    ],

    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "extra": {
      "eas": {
        "projectId": "2ee91b4c-ceb8-4a42-a0d4-6060a00d2ef2"
      }
    }
  }
}
